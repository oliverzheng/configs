text/html;          firefox %s
text/html;          /usr/bin/elinks %s; nametemplate=%s.html
text/html;          /usr/bin/elinks -force-html -dump-width 80 %s; nametemplate=%s.html; copiousoutput

image/*;            feh %s ; test=test -n "$DISPLAY";

text/x-csrc;        vim %s; needsterminal
application/pdf;    evince '%s'; test=test -n "$DISPLAY";
application/msword; soffice %s; copiousoutput
application/vnd.ms-excel; soffice %s; copiousoutput
application/vnd.openxmlformats-officedocument.wordprocessingml.document; soffice %s; copiousoutput
